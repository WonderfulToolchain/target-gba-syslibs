MEMORY
{
        EWRAM   : ORIGIN = 0x020000C8, LENGTH = 256K - 0xC8
        IWRAM   : ORIGIN = 0x03000000, LENGTH =  32K - 512
        VRAM    : ORIGIN = 0x06000000, LENGTH =  96K
}

REGION_ALIAS("ROM", EWRAM)
REGION_ALIAS("EWRAM_LOAD", EWRAM)
REGION_ALIAS("IWRAM_LOAD", IWRAM)
REGION_ALIAS("VRAM_LOAD", VRAM)

PROVIDE(__sp_sys = 0x03007F00);
PROVIDE(__sp_irq = 0x03007FA0);
PROVIDE(__sp_svc = 0x03007FE0);
